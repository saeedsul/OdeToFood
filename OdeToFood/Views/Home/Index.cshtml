@model ResturantsViewModel
@{
    ViewData["Title"] = "OdeToFood";
}

<h2>@Model.CurrentMessage</h2>

@foreach (var restaurant in Model.Resturants)
{
    <div>
        <a asp-action="Edit" asp-route-id=@restaurant.Id>Edit</a>
    </div>
    <div>@restaurant.Name</div>
}

<div><a asp-action="Create">Add Resturant</a></div>